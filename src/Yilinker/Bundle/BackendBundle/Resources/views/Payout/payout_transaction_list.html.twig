    <div class="ui modal modal-box coupled-first coupled-second" id="payout-transaction">
        <a class="close"><i class="icon-times"></i></a>
        <div class="header">Payout Transaction</div>
        <div class="content">
    

            <div class="row">
                    <div class="box-upper">
                        <div class="col-md-12">
                            <div class="box-div">

                                <div class="row gutters">
                                    <div class="col-md-12">
                                        <table class="table basic request-payout-tbl">
                                            <thead>
                                                <tr>
                                                    <th width="10%">Date</th>
                                                    <th width="10%" align="center">Type</th>
                                                    <th width="30%">Description</th>
                                                    <th width="10%" align="right">Amount</th>
                                                    <th width="10%" align="center">Status</th>
                                                </tr>
                                            </thead>
                                            <tbody>
                                                {% for earning in earnings %}
                                                    <tr>
                                                        <td valign="top">{{ earning.dateAdded|date('m/d/Y') }}</td>
                                                        <td align="center" valign="top">{{ earning.earningType }}</td>
                                                        <td valign="top">
                                                            {{ include('YilinkerCoreBundle:Widget:earning_description.html.twig') }}
                                                        </td>
                                                        <td valign="top" align="right">
                                                            {{ earning.amount < 0 ? '-': '+' }} P {{ earning.amount|number_format(2) }}
                                                        </td>
                                                        <td valign="top" align="center">
                                                            {{ earning.status(true) }}
                                                        </td>
                                                    </tr>
                                                {% endfor %}
                                            </tbody>
                                        </table>
                                    </div>
                                </div>

                            </div>
                        </div>

                    </div>

                </div>

            
        </div>
    </div>
