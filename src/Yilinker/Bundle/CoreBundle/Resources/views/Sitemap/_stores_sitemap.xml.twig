<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
{% for store in stores %}
{% if store.storeSlug != '' %}
    <url>
        <loc>{{ path('store_page_products', { 'slug': store.storeSlug }) }}</loc>
        <priority>0.5</priority>
    </url>
{% endif %}
{% endfor %}
</urlset>