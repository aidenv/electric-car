<?php

namespace Yilinker\Bundle\CoreBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * EarningGroupMap
 */
class EarningGroupMap
{
    /**
     * @var integer
     */
    private $earningGroupMapId;

    /**
     * @var \Yilinker\Bundle\CoreBundle\Entity\EarningType
     */
    private $earningType;

    /**
     * @var \Yilinker\Bundle\CoreBundle\Entity\EarningGroup
     */
    private $earningGroup;


    /**
     * Get earningGroupMapId
     *
     * @return integer
     */
    public function getEarningGroupMapId()
    {
        return $this->earningGroupMapId;
    }

    /**
     * Set earningType
     *
     * @param \Yilinker\Bundle\CoreBundle\Entity\EarningType $earningType
     * @return EarningGroupMap
     */
    public function setEarningType(\Yilinker\Bundle\CoreBundle\Entity\EarningType $earningType = null)
    {
        $this->earningType = $earningType;

        return $this;
    }

    /**
     * Get earningType
     *
     * @return \Yilinker\Bundle\CoreBundle\Entity\EarningType
     */
    public function getEarningType()
    {
        return $this->earningType;
    }

    /**
     * Set earningGroup
     *
     * @param \Yilinker\Bundle\CoreBundle\Entity\EarningGroup $earningGroup
     * @return EarningGroupMap
     */
    public function setEarningGroup(\Yilinker\Bundle\CoreBundle\Entity\EarningGroup $earningGroup = null)
    {
        $this->earningGroup = $earningGroup;

        return $this;
    }

    /**
     * Get earningGroup
     *
     * @return \Yilinker\Bundle\CoreBundle\Entity\EarningGroup
     */
    public function getEarningGroup()
    {
        return $this->earningGroup;
    }
}
