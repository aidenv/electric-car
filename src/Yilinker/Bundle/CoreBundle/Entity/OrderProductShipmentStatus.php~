<?php

namespace Yilinker\Bundle\CoreBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * OrderProductShipmentStatus
 */
class OrderProductShipmentStatus
{

    /**
     * @var integer
     */
    private $orderProductShipmentStatusId;

    /**
     * @var string
     */
    private $shipmentStatus;


    /**
     * Get orderProductShipmentStatusId
     *
     * @return integer 
     */
    public function getOrderProductShipmentStatusId()
    {
        return $this->orderProductShipmentStatusId;
    }

    /**
     * Set shipmentStatus
     *
     * @param string $shipmentStatus
     * @return OrderProductShipmentStatus
     */
    public function setShipmentStatus($shipmentStatus)
    {
        $this->shipmentStatus = $shipmentStatus;

        return $this;
    }

    /**
     * Get shipmentStatus
     *
     * @return string 
     */
    public function getShipmentStatus()
    {
        return $this->shipmentStatus;
    }
}
