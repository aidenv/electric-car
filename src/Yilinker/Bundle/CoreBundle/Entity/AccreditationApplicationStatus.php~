<?php

namespace Yilinker\Bundle\CoreBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * AccreditationApplicationStatus
 */
class AccreditationApplicationStatus
{

    const STATUS_OPEN = 1;

    const STATUS_CLOSE = 2;

    /**
     * @var integer
     */
    private $accreditationApplicationStatusId;

    /**
     * @var string
     */
    private $name;

    /**
     * Get accreditationApplicationStatusId
     *
     * @return integer 
     */
    public function getAccreditationApplicationStatusId()
    {
        return $this->accreditationApplicationStatusId;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return AccreditationApplicationStatus
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }

}
