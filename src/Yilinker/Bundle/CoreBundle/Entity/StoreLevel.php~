<?php

namespace Yilinker\Bundle\CoreBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * StoreLevel
 */
class StoreLevel
{

    const STORE_LEVEL_SILVER = 1;

    /**
     * @var integer
     */
    private $storeLevelId;

    /**
     * @var string
     */
    private $name;

    /**
     * @var integer
     */
    private $storeSpace;

    /**
     * @var integer
     */
    private $storeEarning;

    /**
     * Get storeLevelId
     *
     * @return integer 
     */
    public function getStoreLevelId()
    {
        return $this->storeLevelId;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return StoreLevel
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }

    /**
     * Set storeSpace
     *
     * @param integer $storeSpace
     * @return StoreLevel
     */
    public function setStoreSpace($storeSpace)
    {
        $this->storeSpace = $storeSpace;

        return $this;
    }

    /**
     * Get storeSpace
     *
     * @return integer 
     */
    public function getStoreSpace()
    {
        return $this->storeSpace;
    }

    /**
     * Set storeEarning
     *
     * @param integer $storeEarning
     * @return StoreLevel
     */
    public function setStoreEarning($storeEarning)
    {
        $this->storeEarning = $storeEarning;

        return $this;
    }

    /**
     * Get storeEarning
     *
     * @return integer 
     */
    public function getStoreEarning()
    {
        return $this->storeEarning;
    }
}
