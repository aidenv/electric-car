<?php

namespace Yilinker\Bundle\CoreBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * ApplicationRemark
 */
class ApplicationRemarkType
{

    const TYPE_BUSINESS_INFORMATION = 1;

    const TYPE_BANK_INFORMATION = 2;

    const TYPE_FILE_DTI_SEC_PERMIT = 3;

    const TYPE_FILE_MAYORS_PERMIT = 4;

    const TYPE_FILE_BIR_PERMIT = 5;

    const TYPE_FILE_FORM = 6;

    const TYPE_FILE_OTHER = 7;

    const TYPE_FILE_SSS = 8;

    const TYPE_FILE_PAG_IBIG = 9;

    const TYPE_FILE_POSTAL = 10;

    const TYPE_FILE_PASSPORT = 11;

    const TYPE_FILE_DRIVERS_LICENSE = 12;

    const TYPE_FILE_PRC = 13;

    const TYPE_FILE_VOTERS_ID = 14;

    const TYPE_FILE_SCHOOL_ID = 15;

    const TYPE_FILE_TIN = 16;

    const TYPE_VALID_ID = 17;

    /**
     * @var integer
     */
    private $applicationRemarkTypeId;

    /**
     * @var string
     */
    private $name;


    /**
     * Get applicationRemarkTypeId
     *
     * @return integer 
     */
    public function getApplicationRemarkTypeId()
    {
        return $this->applicationRemarkTypeId;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return ApplicationRemarkType
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }
}
