<?php

namespace Yilinker\Bundle\CoreBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

class DisputeDetailVoucher
{
    
    /**
     * @var integer
     */
    private $disputeDetailVoucherId;

    /**
     * @var \Yilinker\Bundle\CoreBundle\Entity\DisputeDetail
     */
    private $disputeDetail;

    /**
     * @var \Yilinker\Bundle\CoreBundle\Entity\VoucherCode
     */
    private $voucherCode;


    /**
     * Get disputeDetailVoucherId
     *
     * @return integer 
     */
    public function getDisputeDetailVoucherId()
    {
        return $this->disputeDetailVoucherId;
    }

    /**
     * Set disputeDetail
     *
     * @param \Yilinker\Bundle\CoreBundle\Entity\DisputeDetail $disputeDetail
     * @return DisputeDetailVoucher
     */
    public function setDisputeDetail(\Yilinker\Bundle\CoreBundle\Entity\DisputeDetail $disputeDetail = null)
    {
        $this->disputeDetail = $disputeDetail;

        return $this;
    }

    /**
     * Get disputeDetail
     *
     * @return \Yilinker\Bundle\CoreBundle\Entity\DisputeDetail 
     */
    public function getDisputeDetail()
    {
        return $this->disputeDetail;
    }

    /**
     * Set voucherCode
     *
     * @param \Yilinker\Bundle\CoreBundle\Entity\VoucherCode $voucherCode
     * @return DisputeDetailVoucher
     */
    public function setVoucherCode(\Yilinker\Bundle\CoreBundle\Entity\VoucherCode $voucherCode = null)
    {
        $this->voucherCode = $voucherCode;

        return $this;
    }

    /**
     * Get voucherCode
     *
     * @return \Yilinker\Bundle\CoreBundle\Entity\VoucherCode 
     */
    public function getVoucherCode()
    {
        return $this->voucherCode;
    }
}
