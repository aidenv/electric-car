<?php

namespace Yilinker\Bundle\CoreBundle\Entity;

use Doctrine\ORM\Mapping as ORM;

/**
 * LegalDocumentType
 */
class LegalDocumentType
{

    const TYPE_DTI_SEC_PERMIT = 1;

    const TYPE_MAYORS_PERMIT = 2;

    const TYPE_BIR_PERMIT = 3;

    const TYPE_FORM_M11501 = 4;

    const TYPE_OTHERS = 5;

    const TYPE_SSS = 6;

    const TYPE_PAG_IBIG = 7;

    const TYPE_POSTAL = 8;

    const TYPE_PASSPORT = 9;

    const TYPE_DRIVERS_LICENSE = 10;

    const TYPE_PRC = 11;

    const TYPE_VOTERS_ID = 12;

    const TYPE_SCHOOL_ID = 13;

    const TYPE_TIN = 14;

    const TYPE_VALID_ID = 15;

    /**
     * @var integer
     */
    private $legalDocumentTypeId;

    /**
     * @var string
     */
    private $name;


    /**
     * Get legalDocumentTypeId
     *
     * @return integer 
     */
    public function getLegalDocumentTypeId()
    {
        return $this->legalDocumentTypeId;
    }

    /**
     * Set name
     *
     * @param string $name
     * @return LegalDocumentType
     */
    public function setName($name)
    {
        $this->name = $name;

        return $this;
    }

    /**
     * Get name
     *
     * @return string 
     */
    public function getName()
    {
        return $this->name;
    }
}
